{
  "xs:schema": {
    "@elementFormDefault": "qualified",
    "@xmlns:xs": "http://www.w3.org/2001/XMLSchema",
    "xs:simpleType": [
      {
        "@name": "CoordinateSystem",
        "xs:restriction": {
          "@base": "xs:string",
          "xs:enumeration": [
            {
              "@value": "CARTESIAN",
              "xs:annotation": {
                "xs:documentation": "Indicates a cartesian coordinate\n\t\t\t\t\t\tsystem."
              }
            },
            {
              "@value": "GEODETIC",
              "xs:annotation": {
                "xs:documentation": "Indicates a geodetic coordinate\n\t\t\t\t\t\tsystem."
              }
            }
          ]
        }
      },
      {
        "@name": "GranuleSpatialRepresentation",
        "xs:annotation": {
          "xs:documentation": "This entity contains the indication of\n\t\t\t\tspatial coordinate system for all the granules in the\n\t\t\t\tcollection."
        },
        "xs:restriction": {
          "@base": "xs:string",
          "xs:enumeration": [
            {
              "@value": "CARTESIAN",
              "xs:annotation": {
                "xs:documentation": null
              }
            },
            {
              "@value": "GEODETIC",
              "xs:annotation": {
                "xs:documentation": null
              }
            },
            {
              "@value": "ORBIT",
              "xs:annotation": {
                "xs:documentation": null
              }
            },
            {
              "@value": "NO_SPATIAL",
              "xs:annotation": {
                "xs:documentation": null
              }
            }
          ]
        }
      },
      {
        "@name": "CommonEntryId",
        "xs:restriction": {
          "@base": "xs:string",
          "xs:minLength": {
            "@value": "1"
          },
          "xs:pattern": {
            "@value": "[^\\\\/:\\s]{1,255}"
          }
        }
      },
      {
        "@name": "ProviderBrowseId",
        "xs:annotation": {
          "xs:documentation": "The unique id for a browse image from the\n\t\t\t\tprovider."
        },
        "xs:restriction": {
          "@base": "xs:string",
          "xs:maxLength": {
            "@value": "255"
          },
          "xs:minLength": {
            "@value": "1"
          }
        }
      },
      {
        "@name": "GranuleUR",
        "xs:annotation": {
          "xs:documentation": "The Universal Reference ID of the granule\n\t\t\t\treferred by the data provider. This ID is unique per data\n\t\t\t\tprovider."
        },
        "xs:restriction": {
          "@base": "xs:string",
          "xs:maxLength": {
            "@value": "250"
          },
          "xs:minLength": {
            "@value": "1"
          }
        }
      },
      {
        "@name": "CollectionShortName",
        "xs:restriction": {
          "@base": "xs:string",
          "xs:maxLength": {
            "@value": "85"
          },
          "xs:minLength": {
            "@value": "1"
          }
        }
      },
      {
        "@name": "Latitude",
        "xs:restriction": {
          "@base": "xs:decimal",
          "xs:minInclusive": {
            "@value": "-90.0"
          },
          "xs:maxInclusive": {
            "@value": "90.0"
          }
        }
      },
      {
        "@name": "Longitude",
        "xs:restriction": {
          "@base": "xs:decimal",
          "xs:minInclusive": {
            "@value": "-180.0"
          },
          "xs:maxInclusive": {
            "@value": "180.0"
          }
        }
      },
      {
        "@name": "DollarAmount",
        "xs:restriction": {
          "@base": "xs:decimal",
          "xs:totalDigits": {
            "@value": "9"
          },
          "xs:fractionDigits": {
            "@value": "2"
          }
        }
      },
      {
        "@name": "MetadataStandardName",
        "xs:restriction": {
          "@base": "xs:string",
          "xs:minLength": {
            "@value": "1"
          },
          "xs:maxLength": {
            "@value": "80"
          }
        }
      },
      {
        "@name": "MetadataStandardVersion",
        "xs:restriction": {
          "@base": "xs:string",
          "xs:minLength": {
            "@value": "1"
          },
          "xs:maxLength": {
            "@value": "80"
          }
        }
      },
      {
        "@name": "ProductFlag",
        "xs:restriction": {
          "@base": "xs:string",
          "xs:enumeration": [
            {
              "@value": "DATA_PRODUCT_FILE"
            },
            {
              "@value": "INSTRUMENT_ANCILLARY_FILE"
            },
            {
              "@value": "SYSTEM/SPACECRAFT_FILE"
            },
            {
              "@value": "EXTERNAL_DATA"
            }
          ]
        }
      }
    ],
    "xs:complexType": [
      {
        "@name": "Line",
        "xs:annotation": {
          "xs:documentation": "This entity holds the horizontal spatial\n\t\t\t\tcoverage of a line. A line area contains at lease two points\n\t\t\t\texpressed with (PointLongitude, PointLatitude). A Line entity\n\t\t\t\tforms with at least two Point entity. ECHO stores horizontal\n\t\t\t\tspatial coverage Line type information using oracle spatial\n\t\t\t\ttype expression."
        },
        "xs:sequence": {
          "xs:element": [
            {
              "@minOccurs": "2",
              "@maxOccurs": "unbounded",
              "@name": "Point",
              "@type": "Point"
            },
            {
              "@minOccurs": "0",
              "@name": "CenterPoint",
              "@type": "Point"
            }
          ]
        }
      },
      {
        "@name": "BoundingRectangle",
        "xs:annotation": {
          "xs:documentation": "This entity holds the horizontal spatial\n\t\t\t\tcoverage of a line. ECHO stores horizontal spatial coverage\n\t\t\t\tbounding rectangle type information using oracle spatial type\n\t\t\t\texpression as a four points polygon. TODO possible merge with\n\t\t\t\tcollection but collection does not have\n\t\t\t\tCenterPoint"
        },
        "xs:sequence": {
          "xs:element": [
            {
              "@name": "WestBoundingCoordinate",
              "@type": "Longitude",
              "xs:annotation": {
                "xs:documentation": "Western-most coordinate of the limit of\n\t\t\t\t\t\tcoverage expressed in longitude\n\t\t\t\t\t\t(degree)."
              }
            },
            {
              "@name": "NorthBoundingCoordinate",
              "@type": "Latitude",
              "xs:annotation": {
                "xs:documentation": "Northern-most coordinate of the limit\n\t\t\t\t\t\tof coverage expressed in latitude\n\t\t\t\t\t\t(degree)."
              }
            },
            {
              "@name": "EastBoundingCoordinate",
              "@type": "Longitude",
              "xs:annotation": {
                "xs:documentation": "Eastern-most coordinate of the limit of\n\t\t\t\t\t\tcoverage expressed in longitude\n\t\t\t\t\t\t(degree)."
              }
            },
            {
              "@name": "SouthBoundingCoordinate",
              "@type": "Latitude",
              "xs:annotation": {
                "xs:documentation": "Southern-most coordinate of the limit\n\t\t\t\t\t\tof coverage expressed in latitude\n\t\t\t\t\t\t(degree)."
              }
            },
            {
              "@minOccurs": "0",
              "@name": "CenterPoint",
              "@type": "Point",
              "xs:annotation": {
                "xs:documentation": "The center point of the bounding\n\t\t\t\t\t\trectangle. This value is ignored for\n\t\t\t\t\t\tcollections."
              }
            }
          ]
        }
      },
      {
        "@name": "GPolygon",
        "xs:annotation": {
          "xs:documentation": "A GPolygon represents an area on the earth\n\t\t\t\trepresented by a main boundary with optional boundaries\n\t\t\t\texcluded regions from the main boundary."
        },
        "xs:sequence": {
          "xs:element": [
            {
              "@name": "Boundary",
              "@type": "Boundary",
              "xs:annotation": {
                "xs:documentation": "The boundary representing the outer\n\t\t\t\t\t\tring of the GPolygon."
              }
            },
            {
              "@minOccurs": "0",
              "@name": "ExclusiveZone",
              "@type": "ExclusiveZone",
              "xs:annotation": {
                "xs:documentation": "Contains the excluded boundaries from\n\t\t\t\t\t\tthe GPolygon."
              }
            },
            {
              "@minOccurs": "0",
              "@name": "CenterPoint",
              "@type": "Point",
              "xs:annotation": {
                "xs:documentation": "The center point of the polygon. This\n\t\t\t\t\t\tvalue is ignored for collections."
              }
            }
          ]
        }
      },
      {
        "@name": "Point",
        "xs:annotation": {
          "xs:documentation": "This entity holds the horizontal spatial\n\t\t\t\tcoverage of a point."
        },
        "xs:sequence": {
          "xs:element": [
            {
              "@name": "PointLongitude",
              "@type": "Longitude",
              "xs:annotation": {
                "xs:documentation": "The longitude value of a spatially\n\t\t\t\t\t\treferenced pointer in degree."
              }
            },
            {
              "@name": "PointLatitude",
              "@type": "Latitude",
              "xs:annotation": {
                "xs:documentation": "The latitude value of a spatially\n\t\t\t\t\t\treferenced pointer in degree."
              }
            }
          ]
        }
      },
      {
        "@name": "ExclusiveZone",
        "xs:annotation": {
          "xs:documentation": "Contains a list of boundaries to exclude\n\t\t\t\tfrom some area."
        },
        "xs:sequence": {
          "xs:element": {
            "@maxOccurs": "unbounded",
            "@name": "Boundary",
            "@type": "Boundary",
            "xs:annotation": {
              "xs:documentation": null
            }
          }
        }
      },
      {
        "@name": "Boundary",
        "xs:annotation": {
          "xs:documentation": "A boundary is set of points connected by\n\t\t\t\tstraight lines representing a polygon on the earth. It takes\n\t\t\t\ta minimum of three points to make a boundary. Points must be\n\t\t\t\tspecified in clockwise order."
        },
        "xs:sequence": {
          "xs:element": {
            "@minOccurs": "3",
            "@maxOccurs": "unbounded",
            "@name": "Point",
            "@type": "Point"
          }
        }
      },
      {
        "@name": "ListOfVerticalSpatialDomains",
        "xs:annotation": {
          "xs:documentation": "This entity contains the domain value and\n\t\t\t\ttype for the granule's vertical spatial\n\t\t\t\tdomain."
        },
        "xs:sequence": {
          "xs:element": {
            "@minOccurs": "0",
            "@maxOccurs": "unbounded",
            "@name": "VerticalSpatialDomain",
            "@type": "VerticalSpatialDomain"
          }
        }
      },
      {
        "@name": "VerticalSpatialDomain",
        "xs:annotation": {
          "xs:documentation": "This entity contains the domain value and\n\t\t\t\ttype for the granule's vertical spatial domain. The reference\n\t\t\t\tframe or system from which altitude or depths are measured.\n\t\t\t\tThe term 'altitude' is used instead of the common term\n\t\t\t\t'elevation' to conform to the terminology in Federal\n\t\t\t\tInformation Processing Standards 70-1 and 173. The\n\t\t\t\tinformation contains the datum name, distance units and\n\t\t\t\tencoding method, which provide the definition for the\n\t\t\t\tsystem."
        },
        "xs:sequence": {
          "xs:element": [
            {
              "@name": "Type",
              "xs:annotation": {
                "xs:documentation": "This attribute describes the type of\n\t\t\t\t\t\tthe area of vertical space covered by the\n\t\t\t\t\t\tlocality."
              },
              "xs:simpleType": {
                "xs:restriction": {
                  "@base": "xs:string",
                  "xs:maxLength": {
                    "@value": "80"
                  },
                  "xs:minLength": {
                    "@value": "1"
                  }
                }
              }
            },
            {
              "@name": "Value",
              "xs:annotation": {
                "xs:documentation": "This attribute describes the extent of\n\t\t\t\t\t\tthe area of vertical space covered by the granule. Must\n\t\t\t\t\t\tbe accompanied by an Altitude Encoding Method\n\t\t\t\t\t\tdescription. The datatype for this attribute is the value\n\t\t\t\t\t\tof the attribute VerticalSpatialDomainType. The unit for\n\t\t\t\t\t\tthis attribute is the value of either DepthDistanceUnits\n\t\t\t\t\t\tor AltitudeDistanceUnits."
              },
              "xs:simpleType": {
                "xs:restriction": {
                  "@base": "xs:string",
                  "xs:maxLength": {
                    "@value": "80"
                  },
                  "xs:minLength": {
                    "@value": "1"
                  }
                }
              }
            }
          ]
        }
      },
      {
        "@name": "RangeDateTime",
        "xs:annotation": {
          "xs:documentation": "This entity stores the start and end\n\t\t\t\tdate/time of a collection."
        },
        "xs:sequence": {
          "xs:element": [
            {
              "@name": "BeginningDateTime",
              "@type": "xs:dateTime",
              "xs:annotation": {
                "xs:documentation": "The time when the temporal coverage\n\t\t\t\t\t\tperiod being described began."
              }
            },
            {
              "@minOccurs": "0",
              "@name": "EndingDateTime",
              "@type": "xs:dateTime",
              "xs:annotation": {
                "xs:documentation": "The time of the temporal coverage\n\t\t\t\t\t\tperiod being described ended"
              }
            }
          ]
        }
      },
      {
        "@name": "ListOfOnlineAccessURLs",
        "xs:annotation": {
          "xs:documentation": "This entity stores the online URL(s) for\n\t\t\t\tthe granule if there is any. Those URL either provides the\n\t\t\t\tsite that user can obtain granule data or give the further\n\t\t\t\tinstruction of obtaining the granule data."
        },
        "xs:sequence": {
          "xs:element": {
            "@minOccurs": "0",
            "@maxOccurs": "unbounded",
            "@name": "OnlineAccessURL",
            "@type": "OnlineAccessURL"
          }
        }
      },
      {
        "@name": "OnlineAccessURL",
        "xs:annotation": {
          "xs:documentation": "This entity stores the online URL(s) for\n\t\t\t\tthe granule if there is any. Those URL either provides the\n\t\t\t\tsite that user can obtain granule data or give the further\n\t\t\t\tinstruction of obtaining the granule data."
        },
        "xs:sequence": {
          "xs:element": [
            {
              "@name": "URL",
              "xs:annotation": {
                "xs:documentation": "If the granule data is available\n\t\t\t\t\t\tonline, then the URL will be provided and recorded\n\t\t\t\t\t\there."
              },
              "xs:simpleType": {
                "xs:restriction": {
                  "@base": "xs:string",
                  "xs:maxLength": {
                    "@value": "1024"
                  },
                  "xs:minLength": {
                    "@value": "1"
                  }
                }
              }
            },
            {
              "@minOccurs": "0",
              "@name": "URLDescription",
              "xs:annotation": {
                "xs:documentation": "Description about the URL or any\n\t\t\t\t\t\tassociated information."
              },
              "xs:simpleType": {
                "xs:restriction": {
                  "@base": "xs:string",
                  "xs:maxLength": {
                    "@value": "4000"
                  }
                }
              }
            },
            {
              "@minOccurs": "0",
              "@name": "MimeType",
              "xs:simpleType": {
                "xs:restriction": {
                  "@base": "xs:string",
                  "xs:maxLength": {
                    "@value": "50"
                  }
                }
              }
            }
          ]
        }
      },
      {
        "@name": "ListOfOnlineResources",
        "xs:sequence": {
          "xs:element": {
            "@minOccurs": "0",
            "@maxOccurs": "unbounded",
            "@name": "OnlineResource",
            "@type": "OnlineResource"
          }
        }
      },
      {
        "@name": "OnlineResource",
        "xs:annotation": {
          "xs:documentation": "This entity records the documentation\n\t\t\t\tinformation of the collection including documentation type\n\t\t\t\tand documentation URL where apply etc."
        },
        "xs:sequence": {
          "xs:element": [
            {
              "@name": "URL",
              "xs:annotation": {
                "xs:documentation": "The URL of the\n\t\t\t\t\t\tresource."
              },
              "xs:simpleType": {
                "xs:restriction": {
                  "@base": "xs:string",
                  "xs:maxLength": {
                    "@value": "1024"
                  },
                  "xs:minLength": {
                    "@value": "1"
                  }
                }
              }
            },
            {
              "@minOccurs": "0",
              "@name": "Description",
              "xs:annotation": {
                "xs:documentation": "Comment about the online\n\t\t\t\t\t\tresource."
              },
              "xs:simpleType": {
                "xs:restriction": {
                  "@base": "xs:string",
                  "xs:maxLength": {
                    "@value": "4000"
                  }
                }
              }
            },
            {
              "@name": "Type",
              "xs:annotation": {
                "xs:documentation": "The type of the resource such as\n\t\t\t\t\t\t'Collection Guide' or 'Campaign Guide'\n\t\t\t\t\t\tetc."
              },
              "xs:simpleType": {
                "xs:restriction": {
                  "@base": "xs:string",
                  "xs:maxLength": {
                    "@value": "50"
                  },
                  "xs:minLength": {
                    "@value": "1"
                  }
                }
              }
            },
            {
              "@minOccurs": "0",
              "@name": "MimeType",
              "xs:annotation": {
                "xs:documentation": "The mime type of the online\n\t\t\t\t\t\tresource."
              },
              "xs:simpleType": {
                "xs:restriction": {
                  "@base": "xs:string",
                  "xs:maxLength": {
                    "@value": "50"
                  }
                }
              }
            }
          ]
        }
      },
      {
        "@name": "ListOfOperationModes",
        "xs:sequence": {
          "xs:element": {
            "@minOccurs": "0",
            "@maxOccurs": "unbounded",
            "@name": "OperationMode",
            "xs:simpleType": {
              "xs:restriction": {
                "@base": "xs:string",
                "xs:maxLength": {
                  "@value": "20"
                },
                "xs:minLength": {
                  "@value": "1"
                }
              }
            }
          }
        }
      },
      {
        "@name": "CollectionRef",
        "xs:annotation": {
          "xs:documentation": "A reference to a collection using one of\n\t\t\t\tthe unique keys in the collection. A collection can be\n\t\t\t\treferenced using a combination of the short name and version\n\t\t\t\tid or the dataset id. Setting only one of short name and\n\t\t\t\tversion id without the other is not a valid collection\n\t\t\t\treference."
        },
        "xs:choice": {
          "xs:sequence": [
            {
              "xs:element": [
                {
                  "@name": "ShortName",
                  "@type": "CollectionShortName",
                  "xs:annotation": {
                    "xs:documentation": "The short name of the collection. The\n\t\t\t\t\t\t\tshort name can be used as a key for the collection when\n\t\t\t\t\t\t\tcombined with the version id."
                  }
                },
                {
                  "@name": "VersionId",
                  "xs:annotation": {
                    "xs:documentation": "The version id of the collection. The\n\t\t\t\t\t\t\tversion id can be used as a unique key for the\n\t\t\t\t\t\t\tcollection when combined with the short\n\t\t\t\t\t\t\tname."
                  },
                  "xs:simpleType": {
                    "xs:restriction": {
                      "@base": "xs:string",
                      "xs:maxLength": {
                        "@value": "80"
                      }
                    }
                  }
                }
              ]
            },
            {
              "xs:element": {
                "@name": "DataSetId",
                "xs:annotation": {
                  "xs:documentation": "The data set id of the\n\t\t\t\t\t\t\tcollection."
                },
                "xs:simpleType": {
                  "xs:restriction": {
                    "@base": "xs:string",
                    "xs:maxLength": {
                      "@value": "1030"
                    },
                    "xs:minLength": {
                      "@value": "1"
                    }
                  }
                }
              }
            },
            {
              "xs:element": {
                "@minOccurs": "1",
                "@maxOccurs": "unbounded",
                "@name": "EntryId",
                "@type": "CommonEntryId"
              }
            }
          ]
        }
      },
      {
        "@name": "EmptyType",
        "xs:annotation": {
          "xs:documentation": "The element should contain no children. In\n\t\t\t\tmost cases, this is used when the presence of the element has\n\t\t\t\tmeaning, however the element itself does not required\n\t\t\t\tdata."
        }
      },
      {
        "@name": "ProviderBrowseElement",
        "xs:annotation": {
          "xs:documentation": "The unique identifier for a browse image\n\t\t\t\tfrom the provider."
        },
        "xs:choice": {
          "xs:element": [
            {
              "@name": "ProviderBrowseId",
              "@type": "ProviderBrowseId"
            },
            {
              "@name": "ProviderBrowseUrl",
              "@type": "ProviderBrowseUrl"
            }
          ]
        }
      },
      {
        "@name": "ProviderBrowseUrl",
        "xs:annotation": {
          "xs:documentation": "The unique url for a browse image from the\n\t\t\t\tprovider."
        },
        "xs:sequence": {
          "xs:element": [
            {
              "@name": "URL",
              "xs:annotation": {
                "xs:documentation": "The URL of the\n\t\t\t\t\t\tbrowse."
              },
              "xs:simpleType": {
                "xs:restriction": {
                  "@base": "xs:string",
                  "xs:maxLength": {
                    "@value": "1024"
                  },
                  "xs:minLength": {
                    "@value": "1"
                  }
                }
              }
            },
            {
              "@name": "FileSize",
              "@type": "xs:long",
              "@minOccurs": "0",
              "xs:annotation": {
                "xs:documentation": "The size of the browse\n\t\t\t\t\t\tfile."
              }
            },
            {
              "@name": "Description",
              "@minOccurs": "0",
              "xs:annotation": {
                "xs:documentation": "The description of the browse\n\t\t\t\t\t\turl."
              },
              "xs:simpleType": {
                "xs:restriction": {
                  "@base": "xs:string",
                  "xs:maxLength": {
                    "@value": "4000"
                  },
                  "xs:minLength": {
                    "@value": "1"
                  }
                }
              }
            },
            {
              "@minOccurs": "0",
              "@name": "MimeType",
              "xs:annotation": {
                "xs:documentation": "The mime type of the browse record."
              },
              "xs:simpleType": {
                "xs:restriction": {
                  "@base": "xs:string",
                  "xs:maxLength": {
                    "@value": "50"
                  }
                }
              }
            }
          ]
        }
      },
      {
        "@name": "ListOfStrings",
        "xs:sequence": {
          "xs:element": {
            "@minOccurs": "0",
            "@maxOccurs": "unbounded",
            "@name": "Item",
            "@type": "xs:string"
          }
        }
      },
      {
        "@name": "ListOfCollectionRefs",
        "xs:annotation": {
          "xs:documentation": "A list of references to\n\t\t\t\tcollections."
        },
        "xs:sequence": {
